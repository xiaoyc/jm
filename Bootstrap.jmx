        <hashTree>
          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor" enabled="true">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import java.security.KeyStore;
import java.security.cert.Certificate;

// Open File(s)
f = new FileOutputStream(&quot;token.csv&quot;, false); 
p = new PrintStream(f);

ArrayList result = vars.getObject(&quot;tokenresult&quot;);

p.println(&quot;deviceid,token,cert&quot;);

for (HashMap row : result) {

	byte[] bytes = (byte[])row.get(&quot;DeviceCertificate&quot;);
	
     InputStream inStream = new ByteArrayInputStream(bytes);
     
	KeyStore keystore = KeyStore.getInstance(KeyStore.getDefaultType());
                
	keystore.load(inStream,&quot;Esoteric$&quot;.toCharArray());
         
	Enumeration enumeration = keystore.aliases();

	String deviceAVPublicKey =&quot;&quot;;
          
	while(enumeration.hasMoreElements()) {
		
	String alias = (String)enumeration.nextElement();
                 
	Certificate certificate = keystore.getCertificate(alias);

	byte[] encodedCertKey = certificate.getEncoded();
	
	deviceAVPublicKey =  Base64.getEncoder().encodeToString(encodedCertKey); //Base64.getMimeEncoder().encodeToString(encodedCertKey);

	}
	
	p.println(row.get(&quot;DeviceId&quot;) + &quot;,&quot; + row.get(&quot;DeviceToken&quot;) + &quot;,&quot; + deviceAVPublicKey);
}

// Close File(s)
p.close();
f.close();</stringProp>
          </BeanShellPostProcessor>
